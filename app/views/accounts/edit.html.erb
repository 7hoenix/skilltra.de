<div class="container-home">
  <% if @user.account_completed == true %>
      <h1 class="heading">Edit Profile</h1>
  <% else %>
    <h1 class="heading">Talk about yourself. Its ok to brag.</h1>
    <% end %>
    <%= render 'form' %>

    <%= link_to 'Show', @account, class: "btn btn-default btn-lg" %>
    <%= link_to 'Back', accounts_path(:id => current_user.id), class: "btn btn-default btn-lg"  %>
</div>